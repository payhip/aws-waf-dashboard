{
    "index_patterns": [
        "awswaf-*"
    ],
    "template": {
        "settings": {
            "number_of_shards": 1,
            "number_of_replicas": "0",
            "default_pipeline": "extract_true_client_ip"
        },
        "mappings": {
            "properties": {
                "clientIp": {
                    "type": "keyword",
                    "fields": {
                        "keyword": {
                            "type": "ip"
                        }
                    }
                },
                "true_client_ip": {
                    "type": "keyword"
                },
                "real_country_code": {
                    "type": "keyword" 
                },
                "timestamp": {
                    "type": "date",
                    "format": "epoch_millis"
                }
            }
        }
    }
}